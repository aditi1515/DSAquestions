import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sn = new Scanner(System.in);
        int line = 1;
        while(sn.hasNextLine()){
            System.out.println(line + " " + sn.nextLine());
            line++;
        }
    }
}
