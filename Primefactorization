import java.util.*;

public class Main{

public static void main(String[] args) {
  Scanner sn = new Scanner(System.in);
  int n = sn.nextInt();
  
  for(int divisor = 2;divisor*divisor<=n ;divisor++){
      while(n % divisor ==0){
          System.out.print(divisor + " ");
          
          n=n/divisor;
      }
  }
      if(n!=1){
          System.out.print(n + " ");
      }
      
 }
}
